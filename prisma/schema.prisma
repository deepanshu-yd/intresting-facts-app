generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model RequestLog {
  id           String   @id @default(cuid())
  userId       String
  topic        String
  responseText String
  isWarning    Boolean  @default(false)
  warningReason String?
  createdAt    DateTime @default(now())

  @@index([userId, createdAt])
}